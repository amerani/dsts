{
  "name": "struct-ts",
  "version": "0.1.2",
  "description": "data structures in typescript",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -d",
    "lint": "tslint src/**/*.ts __tests__/**/*.test.ts",
    "test": "jest"
  },
  "author": "Alek Merani",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.test.ts"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amerani/struct-ts.git"
  },
  "keywords": [
    "typescript",
    "data-structures",
    "linked-list"
  ],
  "bugs": {
    "url": "https://github.com/amerani/struct-ts/issues"
  },
  "homepage": "https://github.com/amerani/struct-ts#readme"
}
